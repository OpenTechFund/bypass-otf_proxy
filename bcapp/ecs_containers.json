[
    {
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "{{log_group}}",
        "awslogs-region": "us-west-2",
        "awslogs-stream-prefix": "ecs"
      }
    },
    "portMappings": [
      {
        "hostPort": 80,
        "protocol": "tcp",
        "containerPort": 80
      }
    ],
    "memory": 123,
    "memoryReservation": 123,
    "cpu": 123,
    "environment": [
      {
        "name": "HTTP_PROTOCOL",
        "value": "https"
      },
      {
        "name": "TARGET_SERVER",
        "value": "{{target_server}}"
      }
    ],
    "image": "{{proxy_image}}",
    "disableNetworking": false,
    "interactive": false,
    "healthCheck": {
      "command": [
        "CMD-SHELL",
        "curl -f http://localhost/ || exit 1"
      ]
    },
    "essential": true,
    "pseudoTerminal": false,
    "readonlyRootFilesystem": false,
    "privileged": false,
    "name": "nginx"
  },
  {
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "{{log_group}}",
        "awslogs-region": "us-west-2",
        "awslogs-stream-prefix": "ecs"
      }
    },
    "cpu": 123,
    "environment": [
      {
        "name": "CF_TUNNEL_CERT_BASE64",
        "value": ""
      },
      {
        "name": "CF_TUNNEL_HOSTNAME",
        "value": "{{bypass_hostname}}"
      },
      {
        "name": "CF_TUNNEL_ORIGIN_URL",
        "value": "localhost:80"
      }
    ],
    "memory": 123,
    "memoryReservation": 123,
    "image": "{{cloudflared_image}}",
    "disableNetworking": false,
    "interactive": false,
    "healthCheck": {
      "command": [
        "CMD-SHELL",
        "curl -f http://localhost/ || exit 1"
      ]
    },
    "essential": true,
    "pseudoTerminal": false,
    "readonlyRootFilesystem": false,
    "privileged": false,
    "name": "cloudflared"
  }
]